<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="JobPreData" verbose="1" preserve-order="true" parallel="false">

    <listeners>
        <listener class-name="com.dcits.util.ExtentTestNGIReporterListenerForGetPre"/>
    </listeners>

    <test name="文件分段JOB预言值获取">
        <parameter name="fileName" value="sonic.properties"/>
        <parameter name="createXmlName" value="ob_resource.xml"/>
        <parameter name="isSepFlag" value="true"/>
        <classes>
            <class name="com.dcits.sonic.interfaceAutoTest.SonicManageDataSolveTest">
                <methods>
                    <include name = "getSonicManageResult"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="普通JOB预言值获取">
        <parameter name="fileName" value="sonicNormal.properties"/>
        <parameter name="createXmlName" value="job_normal.xml"/>
        <parameter name="isSepFlag" value="false"/>
        <classes>
            <class name="com.dcits.sonic.interfaceAutoTest.SonicManageDataSolveTest">
                <methods>
                    <include name = "getSonicManageNormalResult"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>